{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-read",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "shell:allow-open",
    "theme:allow-get-theme",
    "theme:allow-set-theme"
  ]
}
